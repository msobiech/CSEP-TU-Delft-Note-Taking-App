<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import org.kordamp.ikonli.javafx.*?>

<AnchorPane prefHeight="295.0" prefWidth="473.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.controllers.EditCollectionsPopUpCtrl">
   <children>
      <HBox layoutY="-1.0" prefHeight="42.0" prefWidth="474.0" style="-fx-background-color: #FEFAE0;">
         <children>
            <Label alignment="CENTER" prefHeight="42.0" prefWidth="459.0" text="Edit Collections">
               <font>
                  <Font name="Comic Sans MS Bold" size="14.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <HBox layoutY="41.0" prefHeight="254.0" prefWidth="473.0" spacing="20.0">
         <children>
            <VBox prefHeight="265.0" prefWidth="110.0" spacing="5.0" style="-fx-padding: 5px; -fx-background-color: #CCD5AE;">
               <children>
                  <ListView prefHeight="273.0" prefWidth="102.0" style="-fx-background-color: #E0E5B6; -fx-text-fill: #333333;" />
                  <HBox alignment="CENTER" prefHeight="28.0" prefWidth="102.0" spacing="5.0" style="-fx-background-color: #E0E5B6; -fx-text-fill: #333333;">
                     <children>
                        <Button fx:id="addCollectionButton" mnemonicParsing="false" onAction="#addCollection">
                           <graphic>
                              <FontIcon iconLiteral="fa-plus" />
                           </graphic>
                           <styleClass>
                              <String fx:value="button-icon" />
                              <String fx:value="flat" />
                           </styleClass>
                        </Button>
                        <Button mnemonicParsing="false" fx:id="deleteCollectionButton" onAction="#deleteCollection">
                           <graphic>
                              <FontIcon iconLiteral="fa-minus" />
                           </graphic>
                           <styleClass>
                              <String fx:value="button-icon" />
                              <String fx:value="flat" />
                           </styleClass>
                        </Button>
                        <Button mnemonicParsing="false" fx:id="refreshCollectionsButton" onAction="#refreshCollections">
                           <graphic>
                              <FontIcon iconLiteral="fa-rotate-right" />
                           </graphic>
                           <styleClass>
                              <String fx:value="button-icon" />
                              <String fx:value="flat" />
                           </styleClass>
                        </Button>
                     </children>
                  </HBox>
               </children>
            </VBox>
            <VBox prefHeight="254.0" prefWidth="344.0">
               <children>
                  <HBox prefHeight="100.0" prefWidth="364.0" spacing="50.0">
                     <children>
                        <Label prefHeight="100.0" prefWidth="30.0" text="Title">
                           <font>
                              <Font name="Comic Sans MS Bold" size="12.0" />
                           </font>
                        </Label>
                        <TextField prefHeight="26.0" prefWidth="238.0" translateY="37.0" />
                     </children>
                  </HBox>
                  <HBox alignment="BOTTOM_RIGHT" prefHeight="155.0" prefWidth="344.0">
                     <children>
                        <Button alignment="BOTTOM_RIGHT" mnemonicParsing="false" text="Save" translateX="-10.0" translateY="-8.0" />
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</AnchorPane>
